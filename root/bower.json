{
  "name": "{%= name %}",
  "main": [
    "dist/{%= dist_file %}", 
    "dist/{%= dist_file_name %}.min.js"
  ],
  "version": "{%= version %}",
  "homepage": "{%= homepage %}",
  "authors": [
    "{%= author_name %}"
  ],
  "description": "{%= description %}",
  "keywords": [
{% _.forEach(keywords, function(item, index, list) { %}    {%= '"' + item + '"' + (index < list.length - 1 ? ',\n' : '') %}{% }); %}
  ],
  "license": "{%= licenses[0] %}",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    {% if (spm) { %}"sea-modules",
    {% } %}"test",
    "tests"
  ]
}
